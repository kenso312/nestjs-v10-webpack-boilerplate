{
  "name": "nestjs-v9-webpack-boilerplate",
  "version": "2.2.4",
  "description": "Boilerplate for NestJS v9 with Webpack, Pnpm, Fastify, Swagger, Pino Logger, Airbnb JavaScript Guide, Google JSON Style, ESLint, Prettier, Editorconfig, Husky, Lint-Staged, Commitlint, Axios, Docker, Alias Path, Error Handling and Clustering",
  "author": "Ken So",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "nestjs-boilerplate",
    "typescript",
    "webpack",
    "pnpm",
    "fastify",
    "swagger",
    "pino",
    "airbnb-eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kenso312/nestjs-v9-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/kenso312/nestjs-v9-webpack-boilerplate/issues"
  },
  "scripts": {
    "build": "nest build --webpack --webpackPath ./webpack.config.js",
    "dev": "pnpm start:dev",
    "lint": "eslint . --ext js --ext ts --fix && prettier --config ./.prettierrc.js --write **/*.[jt]s --loglevel silent",
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "preinstall": "npx only-allow pnpm",
    "prod": "pnpm start:prod",
    "repl": "pnpm start:repl",
    "start": "NODE_ENV=production node dist/main",
    "start:dev": "nest start --watch",
    "start:prod": "pnpm build && pnpm start",
    "start:repl": "nest start --watch --entryFile ./src/utils/repl",
    "swagger": "ts-node -r tsconfig-paths/register ./src/utils/swagger",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:e2e": "jest --config ./test/jest.e2e.config.ts",
    "test:unit": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/platform-fastify": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fastify": "^4.18.0",
    "joi": "^17.9.2",
    "nestjs-pino": "^3.2.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/types": "^17.4.4",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "5.0.4",
    "webpack": "5.82.1"
  }
}
